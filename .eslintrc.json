{
    "root": true,
    "extends": [
        "./node_modules/eslint-config-defaults/rules/eslint/best-practices/airbnb.js",
        "./node_modules/eslint-config-defaults/rules/eslint/errors/airbnb.js",
        "./node_modules/eslint-config-defaults/rules/eslint/node/airbnb.js",
        "./node_modules/eslint-config-defaults/rules/eslint/strict/airbnb.js",
        "./node_modules/eslint-config-defaults/rules/eslint/style/airbnb.js",
        "./node_modules/eslint-config-defaults/rules/eslint/variables/airbnb.js",
        "./node_modules/eslint-config-defaults/rules/eslint/es6/airbnb.js"
    ],
    "env": {
        "browser": true,
        "node": true,
        "amd": false,
        "mocha": false,
        "jasmine": false,
        "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "impliedStrict": false,
        "ecmaFeatures": null
    },
    "globals": {},
    "rules": {
        "strict": "off",
        "indent": [2, 4, { "SwitchCase": 1 }],
        "comma-dangle": [2, "never"],
        "func-names": [0],
        "no-param-reassign": [0],
        "consistent-return": [0],
        "no-unused-expressions": [0],
        "no-labels": 0,
        "keyword-spacing": ["error", { "before": true, "after": true }],

        "max-len": ["warn", 160],

        "no-empty-label": 0,
        "space-after-keywords": 0,
        "space-return-throw-case": 0
    }
}
